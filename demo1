import { useQuery } from "@tanstack/react-query";
import { getHomeDetails } from "../../data/home/home";
import { AsyncHookData } from "../hook_data";
import { asyncHookData } from "../hook_data_impl";
import { KEY_HOME_DETAILS } from "../keys";
import { HomePageDetails } from "./models";
import { usePlatformModel, usePlatformType } from "../misc/hooks";

export function useHomePageDetails(): AsyncHookData<HomePageDetails> {
  const platformTypeData = usePlatformType().data;
  const platformModelData = usePlatformModel().data;
  const { status, data, error } = useQuery({
    queryKey: [KEY_HOME_DETAILS],
    queryFn: () => getHomeDetails(platformTypeData!, platformModelData!),
    enabled: platformTypeData !== undefined && platformModelData !== undefined,
  });
  return asyncHookData(status, error, data);
}

function extractProperties<T extends SystemWideNetworkConnInfo, K extends keyof T>(data: T[], keys: K[]): Record<K, (T[K] | undefined)[]> {
  const result = {} as Record<K, (T[K] | undefined)[]>;

  // Initialize arrays for each key
  keys.forEach(key => {
    result[key] = [];
  });

  // Fill arrays with values
  data.forEach(item => {
    keys.forEach(key => {
      result[key].push(item[key]);
    });
  });

  return result;
}

// Usage
const extractedData = extractProperties(data, ["alias", "underlyingDevice", "macAddress", "ipv4Address", "recvPackets", "sendPackets", "timeSpan"]);
const aliasArray = extractedData.alias;
const underlyingDeviceArray = extractedData.underlyingDevice;

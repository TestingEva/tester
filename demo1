import { Table } from 'react-bootstrap';
import Footer from '../../../common/footer/Footer';
import './BackhaulLogging.css';
import { BackhaulLoggingConstants } from '../../../constants/Diagnostics';
import BackhaulLoggingTableCell from './BackhaulLoggingTableCell';
import React from 'react';

const WifiExtender = ({ wifiData }) => {
    const refreshLog = () => {};
    const clearLog = () => {};
    const saveLog = () => {};

    return (
        <>
            <div className="backhaul-font-size">
                <Table className="backhaul-table ">
                    <thead>
                        <tr className="b">
                            {BackhaulLoggingConstants.backhaulTableHeader.map((val, index) => (<th key={index}>{val}</th>))}
                        </tr>
                    </thead>
                    <tbody>
                        {Object.entries(wifiData).map(([key, val]) => (
                            <BackhaulLoggingTableCell key={key} dataRow={val}/>
                        ))}
                    </tbody>
                </Table>
                <div className="backhaul-menu-button b">
                    <button className="backhaul-menu-button-1 b" onClick={refreshLog}>{BackhaulLoggingConstants?.refresh}</button>
                    <button className="backhaul-menu-button-1 b" onClick={clearLog}>{BackhaulLoggingConstants?.clear}</button>
                    <button className="backhaul-menu-button-2 b" onClick={saveLog}>{BackhaulLoggingConstants?.save}</button>
                </div>
            </div>
            <Footer />
        </>
    );
};

export default WifiExtender;

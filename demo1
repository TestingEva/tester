import { TypeSelectionBodyOptions, TypeSelectionHeaderContainer } from "./styles";

type Props = {
  currentSelectedType: string;
  availableHeaderOptions: string[];
  onOptionClick: ;
};
export const TypeSelectionHeader: React.FC<Props> = (props) => {
  return (
    <TypeSelectionHeaderContainer>
      {props.availableHeaderOptions.map((val, index) => {
        return (
          <TypeSelectionBodyOptions
            $selected={props.currentSelectedType === val}
            key={index}
            onClick={}
          >
            {val}
          </TypeSelectionBodyOptions>
        );
      })}
    </TypeSelectionHeaderContainer>
  );
};

let logs;

try {
    logs = JSON?.parse(localStorage?.getItem('logs'));
    // Check if logs exist and if all required keys are present
    if (logs && typeof logs === 'object') {
        const requiredKeys = ['info', 'debug', 'warn', 'error', 'events'];
        for (const key of requiredKeys) {
            if (!logs.hasOwnProperty(key)) {
                logs[key] = []; // Initialize missing key with an empty array
            }
        }
    } else {
        // Initialize logs with default values if not present or not an object
        logs = {
            info: [],
            debug: [],
            warn: [],
            error: [],
            events: []
        };
    }
} catch (error) {
    console.error('Error parsing logs from localStorage:', error);
    // Initialize logs with default values in case of error
    logs = {
        info: [],
        debug: [],
        warn: [],
        error: [],
        events: []
    };
}

// Rest of your code remains the same...

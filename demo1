const getMaxJValue = (array) => {
  let maxJ = -Infinity;

  array.forEach((item) => {
    // Extract j from the item using regex to match the pattern
    const match = item.match(/Device\.WiFi\.Radio\.1\.X_SOFTATHOME-COM_ChannelMgt\.ChannelChanges\.(\d+)\.TimeStamp/);
    
    if (match) {
      const jValue = parseInt(match[1], 10);
      if (jValue > maxJ) {
        maxJ = jValue;
      }
    }
  });

  return maxJ;
};

// Example usage:
const array = [
  "Device.WiFi.Radio.1.X_SOFTATHOME-COM_ChannelMgt.ChannelChanges.2.TimeStamp",
  "Device.WiFi.Radio.1.X_SOFTATHOME-COM_ChannelMgt.ChannelChanges.5.TimeStamp",
  "Device.WiFi.Radio.1.X_SOFTATHOME-COM_ChannelMgt.ChannelChanges.3.TimeStamp",
  "Device.WiFi.Radio.1.X_SOFTATHOME-COM_ChannelMgt.ChannelChanges.4.TimeStamp",
  "Device.WiFi.Radio.1.X_SOFTATHOME-COM_ChannelMgt.ChannelChanges.8.TimeStamp",
  "Device.WiFi.Radio.1.X_SOFTATHOME-COM_ChannelMgt.ChannelChanges.6.TimeStamp"
];

const maxJ = getMaxJValue(array);
console.log("Max value of j:", maxJ); // Output: Max value of j: 8

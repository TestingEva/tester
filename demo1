so this is my hook 

export function usePortForwardingRulesInfo(): AsyncHookData<PortForwardingRulesInfo[]> {
  const query = useQuery({
    queryKey: [KEY_PORT_FORWARDING_RULES],
    queryFn: getPortForwardingRules,
  });
  return asyncHookData(query);
}

which i have used in the following component 

import { useNavigate } from "react-router-dom";
import { RouteType } from "../../../domain/misc/models";
import {
  useDeletePortForwardingRuleInfo,
  usePortForwardingRulesInfo,
} from "../../../domain/port-forwarding-rules/hooks";
import { ErrorIndicator } from "../../components/error-indicator/ErrorIndicator";
import { LoadingIndicator } from "../../components/loading-indicator/LoadingIndicator";
import { useNavigator } from "../../components/navigator/navigator";
import { PortForwardingRulesView } from "./PortForwardingRulesView";

export const PortForwardingRules: React.FC = () => {
  const { data, isError, isPending, isFetching } = usePortForwardingRulesInfo();
  const { invoke, isError: isDeletingRulesError } = useDeletePortForwardingRuleInfo();
  const navigate = useNavigate();
  const navigator = useNavigator();

  const onAddNewButtonClick = () => {
    navigator(RouteType.PORT_FWD_RULES_EDIT);
  };

  const onEditClick = (index: number) => {
    navigate("/adv/firewall/portrules/editservice", { state: { index } });
  };

  const onRemoveClick = (id: string) => {
    invoke(id);
  };

  if (isError || isDeletingRulesError) {
    return <ErrorIndicator />;
  }

  if (isPending || isFetching || data === undefined) {
    return <LoadingIndicator />;
  }

  return (
    <PortForwardingRulesView
      data={data}
      onRemoveClick={onRemoveClick}
      onEditClick={onEditClick}
      onAddNewButtonClick={onAddNewButtonClick}
    />
  );
};


const DefaultResults = () => {
  const randomMessage = useMemo(() => {
    return defaultSearchData[Math.floor(Math.random() * defaultSearchData.length)];
  }, []);

  return (
    <Row className="d-flex flex-1">
      <DefaultResult>
        Need some ideas? <br /> Try searching for "{randomMessage}"
      </DefaultResult>
    </Row>
  );
};

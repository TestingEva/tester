import Laptop from './Laptop.svg'
import Bulb from './Bulb.svg'
import Camera from './Camera.svg'

export {Laptop,Bulb,Camera} 


import {Laptop,Bulb} from '../../images/icons/device-icons/exporter'
export const DeviceIcons = ({ type, className }) => {
    const deviceTypeIcons = {
        'Desktop/Laptop': 'Laptop',
        'Bulb':'Bulb'
    }
    const devType = deviceTypeIcons?.[type];

    return (
        <img className={`icon  ${className}`} src={devType || Laptop} alt={devType}/>
    )
}



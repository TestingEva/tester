const lanConnectionNameSchema = z.object({
  "Device.IP.Interface.[Alias == 'lan'].X_PRPL-COM_Description": z.string(),
});

export async function getLanConnectionName(): Promise<{ lanConnectionName: string }> {
  const { token } = await requireSession();
  const response = await getParameterValuesApi(buildRequestForSchema(lanConnectionNameSchema), token);
  const typedResponse = readResponseWithSchema(lanConnectionNameSchema, response);
  return { lanConnectionName: typedResponse["Device.IP.Interface.[Alias == 'lan'].X_PRPL-COM_Description"] };
}

const IGMPResponseSchema = z.object({
  "Device.Services.X_VZ-COM_DeviceConfig.IGMP.EthernetEnable": z.number().min(0).max(1),
  "Device.Services.X_VZ-COM_DeviceConfig.IGMP.MoCAEnable": z.number().min(0).max(1),
  "Device.Services.X_VZ-COM_DeviceConfig.IGMP.WiFi24GEnable": z.number().min(0).max(1),
  "Device.Services.X_VZ-COM_DeviceConfig.IGMP.WiFi5GEnable": z.number().min(0).max(1),
  "Device.Services.X_VZ-COM_DeviceConfig.IGMP.WiFi6GEnable": z.any(),//here
});

export async function getIGMP(): Promise<IGMPInfo> {
  const key = "Device.Services.X_VZ-COM_DeviceConfig.IGMP.";
  const { token } = await requireSession();
  const response = await getParameterValuesApi(buildRequestForKeys([key]), token);
  const typedResponse = readResponseWithSchema(IGMPResponseSchema, response);
  return {
    ethernetEnable: typedResponse["Device.Services.X_VZ-COM_DeviceConfig.IGMP.EthernetEnable"],
    moCAEnable: typedResponse["Device.Services.X_VZ-COM_DeviceConfig.IGMP.MoCAEnable"],
    WiFi24GEnable: typedResponse["Device.Services.X_VZ-COM_DeviceConfig.IGMP.WiFi24GEnable"],
    WiFi5GEnable: typedResponse["Device.Services.X_VZ-COM_DeviceConfig.IGMP.WiFi5GEnable"],
    WiFi6GEnable: typedResponse["Device.Services.X_VZ-COM_DeviceConfig.IGMP.WiFi6GEnable"],
  };
}


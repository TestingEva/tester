const dropDownOptions = useMemo(() => {
    const listIpsSet = new Set(list.map(item => item.excpIPAddress));
    return availableExcpIPAddress.filter(ip => !listIpsSet.has(ip)).map(ip => {
      const objPath = availableDnsDropdownExp.find(item => item.excpIpAddress === ip)?.objPath;
      return { excpIpAddress: ip, objPath };
    });
  }, [list]);
